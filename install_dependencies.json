[
    {
        "dependency": "Rust",
        "arch": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh",
        "debian": "sudo apt update -y && sudo apt install -y cargo && sudo apt install -y rustc",
        "fedora": "sudo yum install rust-toolset-7",
        "gentoo": "sudo emerge --ask dev-lang/rust",
        "nixos": "sudo nix-shell -p rustup",
        "suse": "sudo zypper install rustup"
    },
    {
        "dependency": "TypeScript",
        "a": "npm install -g typescript"
    }
]